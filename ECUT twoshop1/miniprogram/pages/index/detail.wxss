/* miniprogram/pages/index/detail.wxss */

.detail-msg {
  height: 800rpx;
  width: 600rpx;
  margin-left: 25rpx;
  padding-left: 50rpx;
  margin-top: 50rpx;
  background-color: rgb(255, 255, 255);
  border-radius: 50rpx;
  border: 1rpx solid #eeeeee;
}

.detail-msg__list{
  margin-top: 20rpx;
  height: 80rpx;
  border-bottom: 1px solid #eeeeee;
  display: flex;
  flex-direction: row;
  align-items: center;
  padding-bottom: 20rpx;
}
.detail-msg .detail-msg__list image {
 
  width: 50rpx;
  height: 50rpx;
  padding-right: 30rpx;
}
.detail-msg .detail_msg__list .detail_msg__title{
  padding-right: 30rpx;
}
.button{
  display: flex;
  flex-direction: row;
  justify-content: center;
}
button{
  display: inline;
  background-color: rgb(186, 243, 224);
}