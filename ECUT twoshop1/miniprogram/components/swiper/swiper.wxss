.swiperContainer {
  width:750rpx;
  height: 400rpx;
  line-height: 400rpx;  /*行高的值与height相等*/
  text-align: center;
  margin-bottom: 50rpx;
  
}
.swiper_box {
  width: 100%;
  height: 400rpx;
   display: inline-block; /*如果是块级元素需改为行内或行内块级才生效*/
   vertical-align: middle;
  
}
swiper-item image {
  width: 650rpx;
  height: 400rpx;
  border-radius: 50rpx;
}